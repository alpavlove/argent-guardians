{
  "name": "argent",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://alpavlove.github.io/argent-guardians",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate-contracts-typings": "typechain --target ethers-v5 --always-generate-overloads --out-dir ./src/generated './src/abis/*.json'",
    "postinstall": "yarn generate-contracts-typings",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
